{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020", 
      "lib": ["ES2020", "dom"], 
      "sourceMap": true,
      "removeComments": true,
      "strict": true, 
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "strictPropertyInitialization": true,
      "noImplicitThis": true,
      "alwaysStrict":  true,
    }
  }
  